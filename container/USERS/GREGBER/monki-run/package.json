{
  "name": "monki-run",
  "version": "0.0.0",
  "private": true,
  "description": "HTML5 game experience using Pixi.js.",
  "main": "build/monki-run.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/neoziro/monki-run.git"
  },
  "keywords": [
    "game",
    "pixi",
    "html5"
  ],
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoziro/monki-run/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "only": [
            "index.js",
            "src",
            "example"
          ],
          "whitelist": [
            "es6.arrowFunctions",
            "es6.blockScoping",
            "es6.classes",
            "es6.constants",
            "es6.destructuring",
            "es6.modules",
            "es6.parameters",
            "es6.properties.computed",
            "es6.properties.shorthand",
            "es6.spread",
            "es6.tailCall",
            "es6.templateLiterals",
            "react",
            "validation.react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "keyboardjs": "^2.1.1",
    "keystate": "0.0.1",
    "pixi-tiled": "^1.1.2",
    "pixi.js": "^3.0.7"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browser-sync": "^2.9.3",
    "browserify": "^11.1.0",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.0.4",
    "gulp-util": "^3.0.6",
    "minimist": "^1.2.0",
    "node-notifier": "^4.2.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  }
}
