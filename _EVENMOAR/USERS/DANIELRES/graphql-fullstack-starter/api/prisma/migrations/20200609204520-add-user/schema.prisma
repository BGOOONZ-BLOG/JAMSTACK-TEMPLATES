// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("PG_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @default(cuid()) @id
  name      String?
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime?
}
