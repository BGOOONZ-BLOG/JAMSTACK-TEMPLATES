    "gatsby-plugin-benchmark-reporting": "*",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-contentful": "^2.1.88",
    "gatsby-source-drupal": "^3.3.18",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.14",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "prettier": "2.0.4"
