{
  "name": "gatsby-plugin-sass",
  "description": "Gatsby plugin to handle scss/sass files",
  "version": "2.3.12",
  "author": "Daniel Farrell <daniel@mobelux.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
