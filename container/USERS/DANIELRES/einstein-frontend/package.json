{
  "name": "einstein-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test:all": "karma start --singleRun=true && nightwatch -e chrome",
    "test:unit": "karma start",
    "test:e2e": "nightwatch -e chrome",
    "test:e2e-headless": "xvfb-run --server-args='-screen 0, 1366x768x24' npm run test:e2e ",
    "build": "./node_modules/.bin/webpack",
    "postinstall": "./node_modules/.bin/webpack -p",
    "dev": "npm run dev-server & npm run api-faker",
    "dev-server": "FAKE_API_URL=http://localhost:3001 ./node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --content-base build",
    "api-faker": "FAKE_API_URL=http://localhost:3001 node api/fake-api-server.js",
    "lint": "eslint app; eslint tests"
  },
  "author": "",
  "license": "ISC",
