    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.20",
