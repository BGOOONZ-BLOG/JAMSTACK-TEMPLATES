<div class="page-header">
  <%= link_to trees_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_tree_path(@tree), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show tree</h1>
</div>

<dl class="dl-horizontal">
  <dt>Common name:</dt>
  <dd><%= @tree.common_name %></dd>

  <dt>Scientific name:</dt>
  <dd><%= @tree.scientific_name %></dd>

  <dt>Forest:</dt>
  <dd><%= @tree.forest_id %></dd>

</dl>
