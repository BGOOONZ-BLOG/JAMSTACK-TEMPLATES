<div class="page-header">
  <%= link_to forests_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_forest_path(@forest), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show forest</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @forest.name %></dd>

  <dt>Size:</dt>
  <dd><%= @forest.size %></dd>

  <dt>Latitude:</dt>
  <dd><%= @forest.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @forest.longitude %></dd>

  <dt>Climate:</dt>
  <dd><%= @forest.climate %></dd>

</dl>
