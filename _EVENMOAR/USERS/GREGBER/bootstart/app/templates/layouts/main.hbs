</head>
<body {{#if body.className}}class="{{body.className}}"{{/if}}>
  {{> page}}

  {{#if env.development}}
  <script data-main="/assets/js/main" src="/assets/components/requirejs/require.js"></script>
  {{/if}}
  {{#if env.production}}
  <script data-main="/assets/js/main-{{md5 "public/assets/js/main.js"}}" src="/assets/js/require-{{md5 "public/assets/js/require.js"}}.js"></script>
  {{/if}}
