  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "axios": "^0.19.2",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "is-relative-url": "^3.0.0",
    "lodash": "^4.17.15",
    "semver": "^7.3.2",
    "sharp": "^0.25.4",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "babel-preset-gatsby-package": "^0.5.2",
    "cross-env": "^5.2.1"
