    "meant": "^1.0.1",
    "node-fetch": "^2.6.0",
    "opentracing": "^0.14.4",
    "pretty-error": "^2.1.1",
    "progress": "^2.0.3",
    "prompts": "^2.3.2",
    "react": "^16.8.0",
    "redux": "^4.0.5",
    "resolve-cwd": "^3.0.0",
    "semver": "^7.3.2",
