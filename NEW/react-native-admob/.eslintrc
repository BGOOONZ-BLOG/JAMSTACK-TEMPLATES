{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/jsx-handler-names": "error"
  }
}
