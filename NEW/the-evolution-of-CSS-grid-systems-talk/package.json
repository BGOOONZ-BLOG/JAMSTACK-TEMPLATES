{
  "name": "css-grids",
  "version": "1.0.0",
  "description": "My talk about different grid systems",
  "main": "index.js",
  "author": "smakosh",
  "license": "MIT",
  "devDependencies": {
    "mdx-deck": "^4.1.0"
  },
  "scripts": {
    "start": "mdx-deck deck.mdx",
    "build": "npm run now-build",
    "build:presentation": "mdx-deck build deck.mdx --no-html",
    "build:images": "cp -r images dist/",
    "now-build": "yarn run build:presentation && yarn run build:images",
    "deploy": "now"
  },
  "dependencies": {
    "gatsby-source-filesystem": "^2.1.48",
    "styled-components": "^5.0.1"
  }
}
